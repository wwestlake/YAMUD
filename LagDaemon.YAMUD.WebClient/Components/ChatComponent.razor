@using LagDaemon.YAMUD.WebClient.Model

<TabbedViewComponent Tabs="@tabs" />


@code {
    private List<Tab> tabs;
    protected override void OnInitialized()
    {
        tabs = new List<Tab>
        {
            new Tab { Title = "Room Chat", Content = @<RoomChatComponent /> },
            new Tab { Title = "Notifications", Content = @<NotificationsComponent /> },
        };
    }

}
